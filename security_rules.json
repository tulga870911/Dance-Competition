{
  "rules": {
    "emails": {
      ".read": false,
      ".write": "auth != null"
    },
    "textMessages": {
      ".read": false,
      ".write": "auth != null"
    },
    "users": {
      "$userId": {
        ".read": "auth != null && auth.uid == $userId",
        ".write": "auth != null && auth.uid == $userId"
      }
    }
  }
}